-- Table structure for table `wp_shopp_asset`
--

CREATE TABLE `wp_shopp_asset` (
  `id` bigint(20) unsigned NOT NULL AUTO_INCREMENT,
  `data` longblob NOT NULL,
  PRIMARY KEY (`id`)
) ENGINE=MyISAM AUTO_INCREMENT=40 DEFAULT CHARSET=utf8;

