--
-- Table structure for table `wp_shopp_address`
--

CREATE TABLE `wp_shopp_address` (
  `id` bigint(20) unsigned NOT NULL AUTO_INCREMENT,
  `customer` bigint(20) unsigned NOT NULL DEFAULT '0',
  `type` enum('billing','shipping') NOT NULL DEFAULT 'billing',
  `name` varchar(100) NOT NULL DEFAULT '',
  `address` varchar(100) NOT NULL DEFAULT '',
  `xaddress` varchar(100) NOT NULL DEFAULT '',
  `city` varchar(100) NOT NULL DEFAULT '',
  `state` varchar(100) NOT NULL DEFAULT '',
  `country` varchar(2) NOT NULL DEFAULT '',
  `postcode` varchar(10) NOT NULL DEFAULT '',
  `geocode` varchar(16) NOT NULL DEFAULT '',
  `created` datetime NOT NULL DEFAULT '0000-00-00 00:00:00',
  `modified` datetime NOT NULL DEFAULT '0000-00-00 00:00:00',
  PRIMARY KEY (`id`),
  KEY `ref` (`customer`,`type`)
) ENGINE=MyISAM AUTO_INCREMENT=7 DEFAULT CHARSET=utf8;

--
-- Dumping data for table `wp_shopp_address`
--

INSERT INTO `wp_shopp_address` (`id`, `customer`, `type`, `name`, `address`, `xaddress`, `city`, `state`, `country`, `postcode`, `geocode`, `created`, `modified`) VALUES
